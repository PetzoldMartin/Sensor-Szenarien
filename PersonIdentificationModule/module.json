{
    "singleton": false,
    "dependencies": [],
    "category": "",
    "author": "M.Petzold (Aismael)",
    "homepage": "",
    "icon": "icon.png",
    "moduleName": "PersonIdentificationModule",
    "version": "1.0.0",
    "maturity": "stable",
    "defaults": {
        "title": "__m_title__",
        "description": "__m_descr__"
    },
    "schema": {
        "type": "object",
        "properties": {
            "cO2Sensor": {
                "field": "enum",
                "datasource": "namespaces",
                "enum": "namespaces:devices_sensorMultilevel:deviceId"
            },
            "personCounter": {
                "field": "enum",
                "datasource": "namespaces",
                "enum": "namespaces:devices_sensorMultilevel:deviceId",
                "required": true
            }
            ,
            "doorWindowContacts": {
                "type": "array",
                "items": {
                    "field": "enum",
                    "datasource": "namespaces",
                    "enum": "namespaces:devices_sensorBinary:deviceId",
                    "required": false
                }
            },
            "peopleSizeMeasurement": {
                "type": "array",
                "items": {
                    "field": "enum",
                    "datasource": "namespaces",
                    "enum": "namespaces:devices_sensorBinary:deviceId",
                    "required": false
                }
            },
            "personas": {
                "type": "array",
                "items": {
                    "title": "Device",
                    "type": "radio",
                    "enum": ["Man", "Woman", "Child"],
                    "default": "Child",
                    "required": true
                }

            },
            "roomHigh": {
                "type": "integer",
                "required": true
            },
            "roomWidth": {
                "type": "integer",
                "required": true
            },
            "roomLength": {
                "type": "integer",
                "required": true
            },
            "room": {
                "type": "integer",
                "required": true
            },
            "correctionFactor": {
                "type": "integer",
                "required": true
            }
        },
        "required": false
    },
    "options": {
        "collapsible": false,
        "fields": {
            "cO2Sensor": {
                "label": "__l_CO2_Sensor__",
                "type": "select",
                "datasource": "namespaces",
                "field": "optionLabels",
                "optionLabels": "namespaces:devices_sensorMultilevel:deviceName",
                "helper": "__l_CO2_Sensor_help__"
            },
            "personCounter": {
                "label": "__l_PersonenCounter__",
                "type": "select",
                "datasource": "namespaces",
                "field": "optionLabels",
                "optionLabels": "namespaces:devices_sensorMultilevel:deviceName",
                "helper": "__l_PersonenCounter_help__"
            },
            "doorWindowContacts": {
                "label": "__l_DoorWindowContacts__",
                "fields": {
                    "item": {
                        "type": "select",
                        "datasource": "namespaces",
                        "field": "optionLabels",
                        "optionLabels": "namespaces:devices_sensorBinary:deviceName"
                    }
                }
            },
            "peopleSizeMeasurement": {
                "label": "__l_SizeMeasurement__",
                "fields": {
                    "item": {
                        "type": "select",
                        "datasource": "namespaces",
                        "field": "optionLabels",
                        "optionLabels": "namespaces:devices_sensorBinary:deviceName"
                    }
                }
            },
            "personas": {
                "label": "__l_Personas__",
                "type": "array",
                "toolbarSticky": true,
                "items": {
                    "type": "radio"
                }

            },
            "roomHigh": {
                "label": "__l_RoomHigh__",
                "type": "integer",
                "placeholder": "__l_RoomHigh_placeholder__"
            },
            "roomWidth": {
                "label": "__l_RoomWidth__",
                "type": "integer",
                "placeholder": "__l_RoomWidth_placeholder__"
            },
            "roomLength": {
                "label": "__l_RoomLength__",
                "type": "integer",
                "placeholder": "__l_RoomLength_placeholder__"
            },
            "room": {
                "label": "__l_room_id__",
                "type": "integer",
                "helper": "__l_room_help__"
            },
            "correctionFactor": {
                "label": "__l_co_id__",
                "type": "integer",
                "helper": "__l_co_help__"

            }
        }
    }

}